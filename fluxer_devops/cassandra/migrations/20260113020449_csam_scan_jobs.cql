CREATE TABLE IF NOT EXISTS fluxer.csam_scan_jobs (
    job_id text PRIMARY KEY,
    resource_type text,
    bucket text,
    key text,
    cdn_url text,
    filename text,
    content_type text,
    channel_id bigint,
    message_id bigint,
    guild_id bigint,
    user_id bigint,
    status text,
    enqueue_time timestamp,
    last_updated timestamp,
    match_tracking_id text,
    match_details text,
    hashes text,
    error_message text,
    expires_at timestamp
);
